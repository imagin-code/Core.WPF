<?xml version="1.0" encoding="utf-8"?>
<Layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="Imagin.Core.Controls">
  <Top/>
  <Left/>
  <Right>
    <Panel Name="OptionsPanel"/>
  </Right>
  <Bottom/>
  <Root xsi:type="DockLayoutGroup" Orientation="Horizontal">
    <Elements>
      <Element xsi:type="DockLayoutGroup" Orientation="Vertical" Width="6*">
        <Elements>
          <Element xsi:type="DockLayoutDocumentGroup" Height="65*"/>
          <Element xsi:type="DockLayoutPanelGroup" Height="35*">
            <Panels>
              <DockLayoutPanel IsSelected="true" Name="ColorHarmonyPanel"/>
            </Panels>
          </Element>
        </Elements>
      </Element>
      <Element xsi:type="DockLayoutPanelGroup" Width="4*">
        <Panels>
          <DockLayoutPanel IsSelected="true" Name="ColorsPanel"/>
          <DockLayoutPanel Name="ColorIlluminantsPanel"/>
          <DockLayoutPanel Name="ColorMatrixPanel"/>
          <DockLayoutPanel Name="ColorProfilesPanel"/>
        </Panels>
      </Element>
    </Elements>
  </Root>
</Layout>