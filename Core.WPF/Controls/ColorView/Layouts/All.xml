<?xml version="1.0" encoding="utf-8"?>
<Layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="Imagin.Core.Controls">
  <Top/>
  <Left/>
  <Right>
	  <Panel Name="NotificationsPanel"/>
	  <Panel Name="OptionsPanel"/>
	  <Panel Name="ThemePanel"/>
  </Right>
  <Bottom/>
  <Root xsi:type="DockLayoutGroup" Orientation="Horizontal">
    <Elements>
      <Element xsi:type="DockLayoutGroup" Orientation="Vertical" Width="6*">
        <Elements>
          <Element xsi:type="DockLayoutDocumentGroup" Height="65*"/>
          <Element xsi:type="DockLayoutPanelGroup" Height="35*">
            <Panels>
				<DockLayoutPanel IsSelected="true" Name="LogPanel"/>
				<DockLayoutPanel Name="ColorHarmonyPanel"/>
              <DockLayoutPanel Name="ColorAnalysisPanel"/>
            </Panels>
          </Element>
        </Elements>
      </Element>
      <Element xsi:type="DockLayoutPanelGroup" Width="4*">
        <Panels>
          <DockLayoutPanel IsSelected="true" Name="ColorPanel"/>
          <DockLayoutPanel Name="ColorsPanel"/>
          <DockLayoutPanel Name="ColorMatricesPanel"/>
          <DockLayoutPanel Name="ColorProfilesPanel"/>
          <DockLayoutPanel Name="ColorIlluminantsPanel"/>
          <DockLayoutPanel Name="ColorChromacityPanel"/>
          <DockLayoutPanel Name="ColorDifferencePanel"/>
        </Panels>
      </Element>
    </Elements>
  </Root>
</Layout>